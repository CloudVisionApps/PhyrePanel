name: Test PhyrePanel

on:
  push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]

permissions:
  contents: read

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v3

    - name: Install PhyrePanel
      run: |
        wget https://raw.githubusercontent.com/CloudVisionApps/PhyrePanel/main/installers/install.sh
        chmod +x install.sh
        sudo ./install.sh
